<h1>Edit Joke</h1>

<form method="POST" action="/jokes/<%= joke._id %>?_method=PATCH">
    <input value="<%= joke.author %>" name="author" type="text" placeholder="Author" />
    <input value="<%= joke.setUp %>" name="setUp" type="text" placeholder="Setup" />
    <input value="<%= joke.punchLine %>" name="punchLine" type="text" placeholder="Punchline" />
    <button>Update Joke</button>
</form>